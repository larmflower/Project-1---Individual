<h1 class="pariah">Login</h1>

<div class="container">
  <form method="POST" action="/login">
    <div class="form-group row has-success">
      <label class="col-sm-2 col-form-label" for="email">Email</label>
      <div class="col-sm-10">
      <input class="form-control form-control-success" type="text" name="email" id="email" required>
    </div>
  </div>
    <div class="form-group row has-success">
      <label class="col-sm-2 col-form-label" for="password">Password</label>
      <div class="col-sm-10">
      <input class="form-control form-control-success" type="password" name="password" id="password" required>
    </div>
    </div>
    <button class="btn btn-success">Login</button>
      <a href="<%= oauth.github.getLoginURL() %>" class="button is-primary"><span class="icon"><i class="fa fa-github"></i></span><span>Login With Github</span></a>
  </form>
</div>
